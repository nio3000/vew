{
  "name": "vew",
  "version": "0.1.0",
  "description": "A seed project for developing electron apps with vue and webpack.",
  "author": "Leo Deng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myst729/vew.git"
  },
  "main": "entry.js",
  "scripts": {
    "clean": "rimraf bin && rimraf dist",
    "build": "NODE_ENV=production webpack",
    "watch": "NODE_ENV=development webpack --watch",
    "serve": "electron .",
    "pack": "electron-packager ./ 'VEW' --platform=darwin --arch=x64 --out=./binaries --icon=./icon.icns",
    "dev": "npm-run-all clean --parallel watch serve",
    "dist": "npm-run-all clean build pack"
  },
  "keywords": [
    "vue",
    "electron",
    "webpack"
  ],
  "dependencies": {
    "electron-is-dev": "^0.1.2",
    "vue": "^2.1.3",
    "vue-i18n": "^4.7.3",
    "vue-meta": "^0.5.2",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.3",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "electron": "^1.4.13",
    "electron-packager": "^8.4.0",
    "electron-reload": "^1.1.0",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "vue-devtools": "^2.3.1-beta1",
    "vue-loader": "^10.0.1",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.3",
    "webpack": "^1.13.3"
  }
}
